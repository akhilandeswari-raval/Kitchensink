{
  "name": "Kitchensink",
  "version": "0.0.0-development",
  "description": "This is an example app used to showcase Cypress.io End-to-End (E2E) testing. For a full reference of our documentation, go to https://docs.cypress.io",
  "main": "index.js",
  "files": [
    "app",
    "cypress"
  ],
  "scripts": {
    "build": "npm run lint",
    "start": "node ./scripts/start.js",
    "dev": "npm start",
    "test": "npm start & cypress run",
    "pretest": "npm run lint & npm run test and node",
    "print-env": "print-env",
    "lint": "eslint --fix",
    "lint:yaml": "yamllint '*.yml' '.buildkite/*.yml' '.circleci/*.yml' '.github/workflows/*.yml' '.semaphore/*.yml' 'basic/**/*.yml' ",
    "e2e": "cypress run",
    "e2e:record": "cypress run --record",
    "cy:record:chrome": "cypress run --record --browser chrome",
    "cy:record:edge": "cypress run --record --browser edge",
    "cy:record:firefox": "cypress run --record --browser firefox",
    "cy:record:parallel": "cypress run --record --parallel",
    "test:ci": "run-p --race start e2e",
    "test:ci:chrome": "run-p --race start e2e:chrome",
    "test:ci:edge": "run-p --race start e2e:edge",
    "test:ci:firefox": "run-p --race start e2e:firefox",
    "test:ci:record": "run-p --race start e2e:record",
    "test:ci:record:chrome": "run-p --race start e2e:record:chrome",
    "test:ci:record:edge": "run-p --race start e2e:record:edge",
    "test:ci:record:firefox": "run-p --race start e2e:record:firefox",
    "test:ci:record:parallel": "run-p --race start e2e:record:parallel",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info",
    "cy:version": "cypress version",
    "cy:cache:list": "cypress cache list",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "cy:open": "cypress open",
    "local:open": "start-test 8080 cy:open",
    "local:run": "start-test 8080 cy:run",
    "local:run:record": "start-test 8080 cy:run:record",
    "ci:set-port": "node ./scripts/set-port",
    "semantic-release": "semantic-release",
    "prepare": "node .husky/install.mjs",
    "cy:runwithmochawesome": "cypress run --reporter mochawesome",
    "cy:chrome": "cypress run --browser chrome --reporter mochawesome",
    "cy:edge": "cypress run --browser edge --reporter mochawesome",
    "cy:electron": "cypress run --browser electron --reporter mochawesome",
    "reports": "node scripts/report-runs.js",
    "cy:parallel": "npx cypress-parallel -s cy:runwithmochawesome -t 4 -d 'cypress/e2e/*.js'",
    "cy:runwithhtmlreport": "node scripts/report-runs.js",
    "cy:parallelwithhtmlreport": "node scripts/parallel-report-run.js",
    "cy:browsers:parallel": "run-p cy:chrome cy:edge cy:electron",
    "cy:all:browsers:withhtml": "node scripts/cross-browser-run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akhilandeswari-raval/Kitchensink.git"
  },
  "author": "Akhilandeswari Raval",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akhilandeswari-raval/Kitchensink/issues"
  },
  "homepage": "https://github.com/cypress-io/cypress-example-kitchensink#readme",
  "dependencies": {
    "cypress-parallel": "^0.15.0",
    "mochawesome": "7.1.4",
    "mochawesome-merge": "5.1.0",
    "mochawesome-report-generator": "6.3.2",
    "npm-run-all2": "7.0.2",
    "serve": "14.2.5"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.3.1",
    "@bahmutov/print-env": "1.3.0",
    "@eslint/js": "9.39.1",
    "@eslint/json": "0.14.0",
    "@stylistic/eslint-plugin": "5.6.1",
    "cypress": "15.10.0",
    "cypress-downloadfile": "1.2.4",
    "cypress-drag-drop": "1.1.1",
    "eslint": "9.39.1",
    "eslint-plugin-cypress": "5.2.0",
    "eslint-plugin-mocha": "11.2.0",
    "globals": "16.5.0",
    "globby": "11.1.0",
    "husky": "9.0.6",
    "semantic-release": "25.0.3",
    "start-server-and-test": "2.0.11",
    "yaml-lint": "1.7.0"
  },
  "engines": {
    "node": "^20.0.0 || ^22.0.0 || >=24.0.0"
  }
}
